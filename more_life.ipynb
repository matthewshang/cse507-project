    "# Glider discovery\n",
    "solver = z3.Solver()\n",
    "state = make_life(solver, grid_size=6, time_steps=4)\n",
    "for i in range(6):\n",
    "    for j in range(6):\n",
    "        solver.add(state[0][i][j] == state[-1][i][j])\n",
    "solver.add(state[0][1][1] != state[1][1][1])\n",
    "solver.check()\n",
    "model = solver.model()\n",
    "print_model(model, state)"
